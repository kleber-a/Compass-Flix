			"name": "Login",
			"item": [
				{
					"name": "1. Create Request Token",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{API_URL}}/authentication/token/new?api_key={{api_key}}",
							"host": [
								"{{API_URL}}"
							],
							"path": [
								"authentication",
								"token",
								"new"
							],
							"query": [
								{
									"key": "api_key",
									"value": "{{api_key}}"
								}
							]
						}
					},
					"response": []
				},
				{
					"name": "2. Validate Token With Login",
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n  \"username\": \"USERNAME\",\r\n  \"password\": \"PASSWORD\",\r\n  \"request_token\": \"REQUEST_TOKEN_GERADO_NO_REQUEST_ANTERIOR\"\r\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{API_URL}}/authentication/token/validate_with_login?api_key={{api_key}}&language=pt-BR",
							"host": [
								"{{API_URL}}"
							],
							"path": [
								"authentication",
								"token",
								"validate_with_login"
							],
							"query": [
								{
									"key": "api_key",
									"value": "{{api_key}}"
								},
								{
									"key": "language",
									"value": "pt-BR"
								}
							]
						}
					},
					"response": []
				},
				{
					"name": "3. Create session",
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n  \"request_token\": \"REQUEST_TOKEN_VALIDADO_NO_REQUEST_ANTERIOR\"\r\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{API_URL}}/authentication/session/new?api_key={{api_key}}",
							"host": [
								"{{API_URL}}"
							],
							"path": [
								"authentication",
								"session",
								"new"
							],
							"query": [
								{
									"key": "api_key",
									"value": "{{api_key}}"
								}
							]
						}
					},
					"response": []
				}
			]
		},
